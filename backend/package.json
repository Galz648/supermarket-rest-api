{
  "name": "supermarket-query-backend",
  "version": "0.1.0",
  "description": "NestJS Backend REST API for Israeli supermarket data query",
  "type": "module",
  "scripts": {
    "build": "bun run nest build",
    "format": "bun run prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "bun run nest start",
    "start:dev": "bun run nest start --watch",
    "start:debug": "bun run nest start --debug --watch",
    "start:prod": "bun run node dist/main",
    "test": "bun test --timeout 10000",
    "lint": "bun run eslint .",
    "lint:fix": "bun run eslint . --fix",
    "prisma:generate": "bun run prisma generate",
    "prisma:studio": "bun run prisma studio",
    "seed": "bun run prisma/seed.ts"
  },
  "keywords": [
    "supermarket",
    "api",
    "typescript",
    "nestjs",
    "prisma"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@prisma/client": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "nestjs": "^0.0.1",
    "reflect-metadata": "^0.1.13",
    "supertest": "^7.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^11.0.10",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "globals": "^16.0.0",
    "prettier": "^2.8.8",
    "prisma": "^5.0.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.3",
    "typescript-eslint": "^8.25.0"
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ]
}
